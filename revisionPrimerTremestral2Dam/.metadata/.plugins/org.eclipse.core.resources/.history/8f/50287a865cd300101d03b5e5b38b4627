package examenPrsp;

public class PuntoDeEntrada {
	public static void main(String[] args) {
		Estacion estacion = new Estacion();
		for (int i = 0; i < 5; i++) {
			Roboto r = new Roboto(estacion, "Roboto" + (i + 1));
			new Thread(r).start();
		}
		for (int i = 0; i < 2; i++) {
			Supervisor r = new Supervisor(estacion, "Roboto" + (i + 1));
			new Thread(r).start();
		}

	}
}
